{
  "engine_version": 2.0,
  "version": 2.0,
  "url": "https://audiobookbay.lu",
  "name": "audiobookbay",
  "description": "Parser for ABB",
  "supported_categories": {
    "all": "None"
  },
  "search": {
    "no_category": "https://audiobookbay.lu/?s=${query}"
  },
  "download": {
    "internal": {
      "link": {
        "regex_use": "first",
        "regexps": [
          {
            "regex": "href=\"(/abss/[^\"]+)",
            "group": 1,
            "slug_type": "append_url"
          }
        ]
      }
    },
    "regexes": {
      "name": {
        "regex_use": "first",
        "regexps": [
          {
            "regex": "postTitle\"><h1>([^<]+)</h1>",
            "group": 1,
            "slug_type": "complete"
          }
        ]
      },
      "magnet": {
        "regex_use": "all",
        "regexps": [
          {
            "regex": "<td>Info Hash:</td>\\s*<td>(.*?)</td>",
            "group": 1,
            "slug_type": "append_other",
            "other":"magnet:?xt=urn:btih:"
          }
        ]
      }
    }
  }
}
