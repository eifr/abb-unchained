{
  "engine_version": 2.0,
  "version": 2.0,
  "url": "https://audiobookbay.lu",
  "name": "audiobookbay",
  "description": "Parser for ABB",
  "supported_categories": {
    "all": "None"
  },
  "search": {
    "no_category": "${url}/page/${page}/?s=${query}",
    "page_start": 1
  },
  "download": {
    "internal": {
      "link": {
        "regex_use": "first",
        "regexps": [
          {
            "regex": "[^<]+<a\\s+href=\"(https://audiobookbay.lu/asbs[^"]+)",
            "group": 1,
            "slug_type": "complete"
          }
        ]
      }
    },
    "regexes": {
      "name": {
        "regex_use": "first",
        "regexps": [
          {
            "regex": "<h1 itemprop=\"name\">(.*?)</h1>",
            "group": 1,
            "slug_type": "complete"
          }
        ]
      },
      "magnet": {
        "regex_use": "all",
        "regexps": [
          {
            "regex": "<td>Info Hash:</td>\\s*<td>(.*?)</td>",
            "group": 1,
            "slug_type": "append_other",
            "other": "magnet:?xt=urn:btih:"
          }
        ]
      },
      "size": {
        "regex_use": "first",
        "regexps": [
          {
            "regex": "Size:</strong>\\s*<span>([^<]+)</span>",
            "group": 1,
            "slug_type": "complete"
          }
        ]
      }
    }
  }
}
